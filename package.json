{
    "name": "inheritance",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "start": "concurrently \"npm run start-api\" \"npm run start-frontend\"",
        "start-api": "npm --prefix ./api start",
        "start-frontend": "npm --prefix ./frontend start",
        "install-all": "npm install && npm install --prefix ./api && npm install --prefix ./frontend",
        "dev": "concurrently \"npm run dev-api\" \"npm run dev-frontend\"",
        "dev-api": "npm --prefix ./api run dev",
        "dev-frontend": "npm --prefix ./frontend run dev"
    },
    "devDependencies": {
        "concurrently": "^9.1.2"
    },
    "dependencies": {
        "emoji-picker-react": "^4.12.0"
    }
}
